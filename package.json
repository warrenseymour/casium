{
  "name": "casium",
  "version": "2.1.0",
  "description": "Casium â€” An application architecture for React",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/ai-labs-team/casium.git",
  "author": "Nate Abele <natea@advisorinnovationlabs.com>",
  "license": "Apache-2.0",
  "scripts": {
    "peers": "install-self-peers -- --ignore-scripts",
    "build": "tsc",
    "lint": "NODE_ENV=test tslint -c tslint.json \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint-fix": "NODE_ENV=test tslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "test": "./run.sh 'npm run lint' 'npm run unit-test'",
    "dev": "nodemon -e ts,tsx --exec yarn unit-test",
    "unit-test": "nyc mocha -r test/setup.js --opts test/mocha.opts",
    "debug-tests": "mocha -r test/setup.js --opts test/mocha.opts --inspect-brk"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": "^1.1.1",
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^3.1.6",
    "@types/mocha": "^2.2.43",
    "@types/ramda": "types/npm-ramda",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "6.18.2",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-css-modules": "2.6.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
