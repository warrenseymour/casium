    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "chai-enzyme": "0.8.0",
    "cheerio": "1.0.0-rc.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "jss": "8.1.0",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.0-rc.1",
    "nodemon": "^1.14.12",
    "nyc": "^11.4.1",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "1.0.0-beta.2",
    "url-parse": "1.1.9",
    "webpack": "3.5.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "peerDependencies": {
    "axios": "^0.16.0 || ^0.17.0",
    "deep-freeze-strict": "^1.1.0",
    "history": "^4.6.0",
    "js-cookie": "^2.1.4",
    "prop-types": "^15.0.0-0 || ^16.0.0-0",
    "ramda": "^0.25.0",
    "react": "^16.2.0"
  },
  "nyc": {
    "include": [
      "src/**/*.{ts,tsx}"
    ],
    "exclude": [
      "**/*_test.{ts,tsx}"
    ],
    "extension": [
      ".ts",
